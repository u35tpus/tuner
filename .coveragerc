[run]
source = .
omit =
    */test_*.py
    */tests/*
    .venv/*
    __pycache__/*

[report]
precision = 2
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
